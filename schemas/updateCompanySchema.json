{
    "definitions": {},
    "$schema": "http://json-schema.org/draft-07/schema#",
    "$id": "http://example.com/root.json",
    "type": "object",
    "title": "The Root Schema",
    "required": [],
    "properties": {
      "name": {
        "$id": "#/properties/name",
        "type": "string",
        "title": "The Name Schema",
        "default": "",
        "examples": [
          "Facebook"
        ],
        "pattern": "^(.*)$"
      },
      "num_employees": {
        "$id": "#/properties/num_employees",
        "type": "integer",
        "title": "The Num_employees Schema",
        "default": 0,
        "examples": [
          35000
        ]
      },
      "description": {
        "$id": "#/properties/description",
        "type": "string",
        "title": "The Description Schema",
        "default": "",
        "examples": [
          "Social media giant"
        ],
        "pattern": "^(.*)$"
      },
      "logo_url": {
        "$id": "#/properties/logo_url",
        "type": "string",
        "title": "The Logo_url Schema",
        "default": "",
        "examples": [
          "https://image.flaticon.com/icons/png/512/124/124010.png"
        ],
        "pattern": "^(.*)$"
      }
    }
  }